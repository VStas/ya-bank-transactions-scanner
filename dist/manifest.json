{
    "name": "YaBank Scanner",
    "description": "Chrome extension for scanning YaBank transactions and exporting to CSV",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["activeTab", "scripting"],
    "action": {

    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
          "js": ["scripts/parser.js"],
          "matches": [
            "https://bank.yandex.ru/pay/history*"
          ]
        }
      ]
}